<%# <img src="https://images.unsplash.com/photo-1485846234645-a62644f84728?ixid=MnwxMjA3fDB8MHxzZWFyY2h8MXx8bW92aWV8ZW58MHx8MHx8&ixlib=rb-1.2.1&auto=format&fit=crop&w=500&q=60" alt=""> %>
<div class="banner-img">
  <div class="my-navbar">
    <h2>MY MOVIE <br> PLAYLISTS</h2>
  </div>
  <%# <%= image_tag "cinema.jpg", class:'banner-img' %>
</div>



<h2 class="h2">ALL MY LISTS</h2>

<div class="cards">
  <% @lists.each do |list| %>
  <div class="card">
    <h3><%= link_to list.name, list_path(list) %></h3>
  </div>
  <% end %>
</div>

<h2 class="h2">CREATE A NEW LIST</h2>

<div class="form__group field">

  <%= simple_form_for @list do |f| %>
    <%= f.input :name, input_html: { class:'form__field' }, label: false %>
    <%# <label for="name" class="form__label">Name</label> %>
  <% end %>

</div>

  <div class="row">
    <div class="col s12">
      <div class="row">
        <div class="input-field col s12">
          <i class="material-icons prefix">textsms</i>
          <input type="text" id="autocomplete-input" class="autocomplete">
          <label for="autocomplete-input">Autocomplete</label>
        </div>
      </div>
    </div>
  </div>
